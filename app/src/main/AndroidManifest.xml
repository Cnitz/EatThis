<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cnitz.eatthis" >
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ResterauntList"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".StatisticsInput"
            android:label="@string/title_activity_statistics_input" >
        </activity>
        <activity
            android:name=".ReviewList"
            android:label="@string/title_activity_review_list" >

        </activity>
        <activity
            android:name=".ReviewAndStatsMenu"
            android:label="@string/title_activity_review_and_stats_menu" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".StatisticsInput"
            android:label="@string/title_activity_statistics_input" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>
